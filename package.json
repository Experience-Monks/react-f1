{
  "name": "react-f1",
  "version": "5.0.2",
  "description": "React UI animation components built on top of f1-dom and f1.",
  "main": "index.js",
  "license": "MIT",
  "author": {
    "name": "Mikko Haapoja",
    "email": "me@mikkoh.com",
    "url": "https://github.com/mikkoh"
  },
  "scripts": {
    "start": "npm run browser",
    "test": "npm run node",
    "example-f1": "budo example/f1/index.js --live --open",
    "example-chief": "budo example/chief/index.js --live --open",
    "browser": "npm run dev-browser",
    "node": "babel-node test/index.js",
    "dev-browser": "budo test/index.js --live --open",
    "dev-node": "nodemon --exec \"babel-node test/index.js | faucet\""
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "dependencies": {
    "deep-extend": "^0.4.0",
    "f1": "^7.0.2",
    "f1-dom": "^7.1.3",
    "gl-mat4": "^1.1.4"
  },
  "peerDependencies": {
    "react": "^0.14.7",
    "react-dom": "^0.14.3"
  },
  "devDependencies": {
    "async": "^1.5.2",
    "babel-cli": "^6.5.1",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.2.0",
    "budo": "^5.1.5",
    "dom-select": "^1.1.0",
    "eases": "^1.0.8",
    "faucet": "0.0.1",
    "jsdom": "^8.0.2",
    "nodemon": "^1.8.1",
    "tape": "^4.4.0"
  },
  "keywords": [
    "react,f1,animation,ui,jam3,dom,react-dom"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/Jam3/react-f1.git"
  },
  "homepage": "https://github.com/Jam3/react-f1",
  "bugs": {
    "url": "https://github.com/Jam3/react-f1/issues"
  }
}
